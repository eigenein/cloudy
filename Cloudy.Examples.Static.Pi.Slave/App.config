<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="console" xsi:type="ColoredConsole" layout="${level}: ${message}" />
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="console" />
    </rules>
  </nlog>
  <appSettings>
    <add key="MasterAddress" value="127.0.0.1"/>
    <add key="MasterPort" value="2011"/>
    <add key="LocalAddress" value="127.0.0.1"/>
    <add key="BaseLocalPort" value="2012"/>
    <add key="SlotsCount" value="1"/>
  </appSettings>
</configuration>